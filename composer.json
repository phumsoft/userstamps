{
    "name": "phumsoft/userstamps",
    "description": "Laravel Userstamps provides an Eloquent trait which automatically maintains `created_by` and `updated_by` columns on your model, populated by the currently authenticated user in your application.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "userstamps",
        "created_by",
        "updated_by",
        "deleted_by"
    ],
    "authors": [
        {
            "name": "Phumsoft",
            "email": "info@phumsoft.com",
            "homepage": "https://www.phumsoft.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.2|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "illuminate/database": "^5.2|^6.0|^7.0|^8.0",
        "orchestra/testbench": "^3.1|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Phumsoft\\Userstamps\\": "src"
        }
    }
}
